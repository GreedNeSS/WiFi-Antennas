@{
    Dictionary<int, int> channels = new Dictionary<int, int>()
    {
        { 31, 2414 },
        { 32, 2419 },
        { 33, 2424 },
        { 34, 2429 },
        { 35, 2434 },
        { 36, 2439 },
        { 37, 2444 },
        { 38, 2449 },
        { 39, 2454 },
        { 40, 2459 },
        { 41, 2464 },
    };
}
<div class="card text-bg-light mb-3" ">
    <div class="card-header">Каналы Wi-Fi 2,4 ГГц</div>
    <div class="card-body">
        <h5 class="card-title">Список частот для старших каналов:</h2>
        <table class="table table-hover table-striped">
            <tr>
                <th>
                    Номер канала
                </th>
                <th>
                    Частота канала
                </th>
            </tr>

            @foreach (var channel in channels)
            {
                <tr><td>@channel.Key</td><td>@channel.Value</td></tr>
            }

        </table>
    </div>
</div>